{
  "directories": {
    "output": "dist"
  },
  "publish": [
    {
      "provider": "github",
      "releaseType": "prerelease"
    }
  ],
  "files": [
    "**/*",
    "!config",
    "!src/renderer",
    "!webpack.config.js"
  ],
  "mac": {
    "category": "public.app-category.lifestyle",
    "target": [
      "dmg"
    ]
  },
  "win": {
    "target": [
      "nsis"
    ]
  }
}
